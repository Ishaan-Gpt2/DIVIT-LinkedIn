# Chaitra Backend Environment Variables

# API Keys for External Services
GEMINI_API_KEY=your_gemini_api_key_here
UNDETECTABLE_API_KEY=your_undetectable_api_key_here
SAPLING_API_KEY=your_sapling_api_key_here
RESEND_API_KEY=your_resend_api_key_here
APIFY_API_KEY=your_apify_api_key_here
PHANTOMBUSTER_API_KEY=your_phantombuster_api_key_here
LANGUAGETOOL_API_KEY=your_languagetool_api_key_here
UPLOAD_POST_API_KEY=your_upload_post_api_key_here

# PhantomBuster Configuration
PHANTOM_ID=your_phantom_id_here

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Frontend Environment Variables (for Vite)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_UNDETECTABLE_API_KEY=your_undetectable_api_key_here
VITE_SAPLING_API_KEY=your_sapling_api_key_here
VITE_RESEND_API_KEY=your_resend_api_key_here
VITE_APIFY_API_KEY=your_apify_api_key_here
VITE_PHANTOMBUSTER_API_KEY=your_phantombuster_api_key_here
VITE_LANGUAGETOOL_API_KEY=your_languagetool_api_key_here
VITE_UPLOAD_POST_API_KEY=your_upload_post_api_key_here
VITE_API_BASE_URL=/api

# Supabase Configuration (if using Supabase)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here